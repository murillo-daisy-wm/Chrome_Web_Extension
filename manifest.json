{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup.html"
    "default_icon": "wild-daisy.jpg"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
  "content_scripts": [
{
  "matches": [
    "<all_urls>"
  ],
  "js": [
    "script.js"
  ]
}
],

}
}
